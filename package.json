	"license": "MIT",
	"bugs": {
		"url": "https://github.com/stake-my-sol/stake-my-sol-api/issues"
	},
	"homepage": "https://github.com/stake-my-sol/stake-my-sol-api#readme",
	"dependencies": {
		"@solana/web3.js": "^1.35.1",
		"axios": "^0.26.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.17.2",
		"lodash": "^4.17.21",
		"mongodb": "^4.3.1",
		"mongoose": "^6.11.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.6",
		"@babel/core": "^7.17.5",
		"@babel/node": "^7.16.8",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/polyfill": "^7.12.1",
		"@babel/preset-env": "^7.16.11",
		"@babel/runtime": "^7.17.2",
		"eslint": "^8.10.0",
		"gulp-nodemon": "^2.5.0",
		"nodemon": "^2.0.0"
	}
